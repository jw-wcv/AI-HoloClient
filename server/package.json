{
  "name": "virtual-agent-system",
  "version": "1.0.0",
  "description": "A system of virtual agents performing tasks with actionable outputs.",
  "main": "server.js",
  "scripts": {
    "start": "concurrently --kill-others-on-fail \"npm:watch-server\" \"npm:run-mongodb\"",
    "remove-lock": "rm ./database/mongodb-data/mongod.lock",
    "repair-db": "mongod --dbpath ./database/mongodb-data --repair",
    "rebuild-db": "rm -rf ./database/mongodb-data && mkdir ./database/mongodb-data && npm start",
    "watch-server": "nodemon server.js",
    "run-mongodb": "mongod --dbpath ./database/mongodb-data",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ffmpeg/core": "^0.12.6",
    "@ffmpeg/ffmpeg": "^0.12.10",
    "@octokit/rest": "^20.0.2",
    "archiver": "^7.0.1",
    "axios": "^1.6.3",
    "bcrypt": "^5.1.1",
    "bull": "^3.29.3",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "deepspeech": "^0.9.3",
    "dotenv": "^16.0.0",
    "excel4node": "^1.8.2",
    "exceljs": "^4.4.0",
    "express": "^4.18.1",
    "express-fileupload": "^1.4.3",
    "express-rate-limit": "^6.3.0",
    "express-validator": "^6.14.0",
    "extract-zip": "^2.0.1",
    "form-data": "^4.0.0",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.8",
    "nodemon": "^2.0.15",
    "openai": "^4.17.4",
    "readable": "^1.1.3",
    "unzipper": "^0.12.3",
    "uuid": "^9.0.1",
    "vosk": "^0.3.39",
    "ws": "^8.16.0",
    "xml2js": "^0.6.2"
  }
}
